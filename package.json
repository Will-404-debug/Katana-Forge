    "@react-three/fiber": "^8.15.16",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.33",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.65.0",
    "resend": "^6.2.2",
    "stripe": "^19.1.0",
    "three": "^0.165.0",
    "zod": "^3.23.8"
