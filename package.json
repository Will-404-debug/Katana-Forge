{
  "name": "katana-forge",
  "version": "1.8.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "db:apply": "prisma db execute --file prisma/migrations/0001_init/migration.sql --schema prisma/schema.prisma",
